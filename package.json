{
  "name": "easy-js-cache",
  "version": "1.0.8",
  "description": "Easy caching data",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/**/*"
  ],
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "scripts": {
    "patch": "npm version patch && npm publish",
    "minor": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "typescript",
    "ts",
    "caching",
    "data",
    "local",
    "cache",
    "api",
    "services",
    "javascript",
    "js",
    "react",
    "angular",
    "vue",
    "svelte"
  ],
  "author": "eyeseeice",
  "license": "MIT",
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.7.0",
    "@parcel/compressor-gzip": "^2.7.0",
    "@parcel/optimizer-esbuild": "^2.7.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  }
}
